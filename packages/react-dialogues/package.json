{
  "name": "react-dialogues",
  "version": "1.0.4",
  "description": "Tiny interaction React UI library",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "vite build && vite build --emptyOutDir false -- --nocss",
    "lint": "eslint src '*.js' '*.mjs' && prettier --check .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepack": "npm run build"
  },
  "repository": "megahertz/react-dialogues",
  "files": [
    "dist/*"
  ],
  "keywords": [
    "react",
    "modal"
  ],
  "author": "Alexey Prokhorov",
  "license": "MIT",
  "bugs": "https://github.com/megahertz/react-dialogues/issues",
  "homepage": "https://github.com/megahertz/react-dialogues#readme",
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite": "^6.0.3",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^4.1.0",
    "vitest": "^3.0.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  }
}
